local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "iy",
	Callback = function()
  loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  	end    
})

Tab:AddButton({
	Name = "fling",
	Callback = function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/0Ben1/fe./main/Fling%20GUI"))()
  	end    
})

Tab:AddButton({
	Name = "acs",
	Callback = function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/1Devz/scripts/main/DevZ%20Hub"))()
  	end    
})
Tab:AddButton({
	Name = "gravity",
	Callback = function()
  loadstring(game:HttpGet('https://pastebin.com/raw/L8QbQ8Qu'))()
  	end    
})

ExploitsTab:AddToggle({
	Name = "Kill Aura", Default = false, Save = true, Flag = "killaura_pl",
	Callback = function(bool)
		getgenv().ka = bool
        if bool then
            killAura()
        end
	end    
})
